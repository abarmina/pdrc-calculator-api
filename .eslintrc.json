{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "browser": false
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "globals": {
    "strapi": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "prettier/prettier": "error"
  }
}